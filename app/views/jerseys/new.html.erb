<%= form_with(model: @jersey, local: true) do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :year %>
  <%= form.number_field :year %>

  <%= form.label :size %>
  <%= form.text_field :size %>

  <%= form.label :photo %>
  <%= form.file_field :photo %>

  <%= form.hidden_field :team_id, value: @team.id %>

  <%= form.submit "Créer l’annonce" %>
<% end %>
